<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.kalda.domain.TblReplyJavaRanch" table="TblReplyJavaRanch">
<id name="id" type="java.lang.Integer">
<column name="id" />
<generator class="native" />
</id>
<property name="replyContent" type="java.lang.String">
  <column name="replyContent" />
</property>
<property name="author" type="java.lang.String">
  <column name="author" />
</property>
<property name="replySequenceNum" type="java.lang.Integer">
  <column name="replySequenceNum" />
</property>
<property name="threadId" type="java.lang.Integer">
  <column name="threadId" />
</property>
</class>
</hibernate-mapping>
