<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.kalda.domain.TblThreadJavaRanch" table="TblThreadJavaRanch">
<id name="id" type="java.lang.Integer">
<column name="id" />
<generator class="native" />
</id>
<property name="title" type="java.lang.String">
  <column name="title" />
</property>
<property name="threadId" type="java.lang.Integer">
  <column name="threadId" />
</property>
<property name="threadURL" type="java.lang.String">
  <column name="threadURL" />
</property>
<property name="answerAmount" type="java.lang.Integer">
  <column name="answerAmount" />
</property>
<property name="component" type="java.lang.String">
  <column name="component" />
</property>
<property name="author" type="java.lang.String">
  <column name="author" />
</property>
</class>
</hibernate-mapping>
