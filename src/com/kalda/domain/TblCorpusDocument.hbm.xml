<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.kalda.domain.TblCorpusDocument" table="TblCorpusDocument">
<id name="id" type="java.lang.Integer">
<column name="id" />
<generator class="native" />
</id>
<property name="documentName" type="java.lang.String">
  <column name="documentName" />
</property>
<property name="documentContent" type="java.lang.String">
  <column name="documentContent" />
</property>
<property name="documentComment" type="java.lang.String">
  <column name="documentComment" />
</property>
</class>
</hibernate-mapping>
