<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.kalda.domain.TblUser" table="TblUser">
<id name="id" type="java.lang.Integer">
<column name="id" />
<generator class="native" />
</id>
<property name="whichCorpus" type="java.lang.String">
  <column name="whichCorpus" />
</property>
<property name="userName" type="java.lang.String">
  <column name="userName" />
</property>
<property name="postAmount" type="java.lang.Integer">
  <column name="postAmount" />
</property>
<property name="replyAmount" type="java.lang.Integer">
  <column name="replyAmount" />
</property>
</class>
</hibernate-mapping>
